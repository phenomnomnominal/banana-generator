{
  "name": "banana-generator",
  "description": "Basic ANgulAr component iNformAtion generator.",
  "version": "0.1.0",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/banana-generator.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/banana-generator/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/src/index.js",
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "compile": "tsc",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'test/**/*.ts'",
    "lint:fix": "npm run lint:src:fix && npm run lint:test",
    "lint:src:fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "lint:test:fix": "tslint -c tslint.json 'test/**/*.ts' --fix",
    "test": "nyc mocha './test/**/**.spec.ts'",
    "build": "npm run compile && npm run lint && npm run test",
    "prepublishOnly": "rm -rf dist && npm run build",
    "watch": "npm-watch"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/sinon": "^4.3.0",
    "chai": "^4.1.2",
    "ineeda": "^0.11.1",
    "mocha": "^3.5.3",
    "npm-watch": "^0.1.8",
    "nyc": "^11.4.1",
    "sinon": "^2.1.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/fs-extra": "^5.0.1",
    "@types/glob": "^5.0.35",
    "cheerio": "^1.0.0-rc.2",
    "fs-extra": "^3.0.0",
    "glob": "^7.1.1",
    "injection-js": "^2.2.1",
    "minimist": "^1.2.0",
    "npmlog": "^4.0.2",
    "reflect-metadata": "^0.1.12",
    "typescript": "^2.3.2"
  },
  "bin": {
    "banana": "./bin/banana"
  },
  "nyc": {
    "include": [
      "**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "report-dir": "./reports/coverage/",
    "sourceMap": true,
    "instrument": true
  }
}
